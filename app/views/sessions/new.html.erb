<h3>Login!</h3>
<br></br>
<div class="col-md-4 col-md-offset-4">
  <div class='NewLogin'>
      <%= bootstrap_form_for @user, url: create_session_path do |f| %>
      <%= f.text_field :email, placeholder: 'Email' %>
      <%= f.password_field :password, placeholder: 'Password' %>
      <br>
    <div class='loginbutton'>
      <%= f.submit "Login", :class=>"btn btn-primary btn-md"%><br><br>
      <p>Don't have an account? Please
      <%= link_to "sign up!", new_user_path %></p>
      <% end %>
      <br><br>
    </div>
  </div>
</div>
